// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id String @id @default(uuid())
  name String
  email String
  cpf String
  password String
  createdAt DateTime @default(now())
}

model Seller {
  id String @id @default(uuid())
  name String
  email String
  cnpj String
  password String
createdAt DateTime @default(now())
}